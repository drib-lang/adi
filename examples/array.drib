val arr = array("0", "1", "2", "3");
println(arr); // array(4) ["0", "1", "2", "3"]

pop(arr);
println(arr); // array(3) ["0", "1", "2"]

shift(arr);
println(arr); // array(2) ["1", "2"]

push(arr, "9");
println(arr); // array(3) ["1", "2", "9"]

unshift(arr, "8");
println(arr); // array(4) ["8", "1", "2", "9"]
